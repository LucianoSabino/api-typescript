{
    "name": "api",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --watch \"src/\" --exec \"node_modules/.bin/ts-node src/index.ts\" -e ts",
        "knex:rollback-all": "knex --knexfile ./src/server/database/Knex/knexfile.ts migrate:rollback --all",
        "knex:rollback": "knex --knexfile ./src/server/database/Knex/knexfile.ts migrate:rollback",
        "knex:migrate": "knex --knexfile ./src/server/database/Knex/knexfile.ts migrate:latest",
        "knex:seed": "knex --knexfile ./src/server/database/Knex/knexfile.ts seed:run",
        "test": "jest"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^22.8.4",
        "@types/supertest": "^6.0.2",
        "dotenv": "^16.4.5",
        "eslint": "^9.13.0",
        "globals": "^15.11.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsc": "^2.0.4",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.12.2"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "express": "^4.21.1",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "sqlite3": "^5.1.7",
        "yup": "^1.4.0"
    },
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ESNext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true
    },
    "include": [
        "src"
    ]
}
